{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run server",
    "server": "node ./dist/server.js",
    "build": "concurrently  \"npm run webpack\" \"npm run tsc\" \"npm run copy\"",
    "webpack": "webpack --config webpack.config.js",
    "tsc": "tsc -p tsconfig-server.json",
    "copy": "node copy-static-assert.js",
    "copy-w": "node copy-static-assert.js -w",
    "mongo": "node start-mongodb.js"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "crypto": "^0.0.3",
    "express": "^4.16.2",
    "express-decorator": "^1.1.0",
    "express-session": "^1.14.2",
    "mongoose": "^4.7.4",
    "showdown": "^1.8.1",
    "vue": "^2.5.3",
    "vue-class-component": "^6.1.0",
    "vue-resource": "^1.3.4",
    "vue-router": "^2.8.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.36",
    "@types/express-session": "^1.15.0",
    "@types/mongoose": "^4.7.18",
    "@types/node": "^8.0.10",
    "@types/serve-favicon": "^2.2.28",
    "chokidar": "^1.7.0",
    "concurrently": "^3.5.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "shelljs": "^0.7.8",
    "vue-loader": "^10.3.0",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^2.7.0"
  }
}
